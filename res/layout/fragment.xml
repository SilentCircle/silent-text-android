<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" style="@style/Widget.Silence.RelativeLayout">
	<FrameLayout android:id="@+id/content" android:layout_width="fill_parent" android:layout_height="fill_parent"/>
	<TextView style="@style/Widget.Silence.TextView.Error" android:id="@+id/error" android:layout_width="fill_parent" android:layout_height="wrap_content" android:gravity="center_horizontal"/>
	<include android:id="@+id/export" layout="@layout/upload" android:visibility="gone" android:layout_alignParentBottom="true" android:layout_height="wrap_content" android:layout_width="fill_parent"/>
	<include android:id="@+id/progress" layout="@layout/indeterminate_progress" android:visibility="gone"/>
	<View android:id="@+id/blackout" style="@style/Widget.Silence.Blackout"/>
</RelativeLayout>
