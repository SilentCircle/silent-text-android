<?xml version='1.0' encoding='UTF-8'?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <plurals name="notify_new_messages_title">
    <item quantity="zero"><xliff:g id="quantity">%1$d</xliff:g> mensagens novas</item>
    <item quantity="one">Nova mensagem recebida</item>
    <item quantity="two"><xliff:g id="quantity">%1$d</xliff:g> mensagens novas</item>
    <item quantity="few"><xliff:g id="quantity">%1$d</xliff:g> mensagens novas</item>
    <item quantity="many"><xliff:g id="quantity">%1$d</xliff:g> mensagens novas</item>
    <item quantity="other"><xliff:g id="quantity">%1$d</xliff:g> mensagens novas</item>
  </plurals>

  <plurals name="notify_new_messages_subtitle">
    <item quantity="zero"><xliff:g id="number_of_people">%1$d</xliff:g> pessoas enviaram a você <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
    <item quantity="one"><xliff:g id="number_of_people">%1$d</xliff:g> pessoa enviou a você <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
    <item quantity="two"><xliff:g id="number_of_people">%1$d</xliff:g> pessoas enviaram a você <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
    <item quantity="few"><xliff:g id="number_of_people">%1$d</xliff:g> pessoas enviaram a você <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
    <item quantity="many"><xliff:g id="number_of_people">%1$d</xliff:g> pessoas enviaram a você <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
    <item quantity="other"><xliff:g id="number_of_people">%1$d</xliff:g> pessoas enviaram a você <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
  </plurals>

  <plurals name="n_messages">
      <item quantity="zero"><xliff:g id="count">%1$d</xliff:g> mensagens</item>
      <item quantity="one">uma mensagem</item>
      <item quantity="two"><xliff:g id="count">%1$d</xliff:g> mensagens</item>
      <item quantity="few"><xliff:g id="count">%1$d</xliff:g> mensagens</item>
      <item quantity="many"><xliff:g id="count">%1$d</xliff:g> mensagens</item>
      <item quantity="other"><xliff:g id="count">%1$d</xliff:g> mensagens</item>
  </plurals>

  <plurals name="notify_unread_messages_from_individual">
    <item quantity="zero"><xliff:g id="count">%1$d</xliff:g> mensagens não lidas de <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="one"><xliff:g id="count">%1$d</xliff:g> mensagem não lida de <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="two"><xliff:g id="count">%1$d</xliff:g> mensagens não lidas de <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="few"><xliff:g id="count">%1$d</xliff:g> mensagens não lidas de <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="many"><xliff:g id="count">%1$d</xliff:g> mensagens não lidas de <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="other"><xliff:g id="count">%1$d</xliff:g> mensagens não lidas de <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
  </plurals>
  
  <plurals name="short_time_seconds">
