		<item name="android:orientation">horizontal</item>
	</style>

	<style name="Widget.Layout.Footer" parent="Widget.Layout.Linear.Vertical">
		<item name="android:layout_alignParentBottom">true</item>
		<item name="android:background">@drawable/bg_footer_dark</item>
	</style>

	<style name="Divider">
		<item name="android:background">@drawable/bg_divider</item>
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">@dimen/stroke_normal</item>
	</style>

	<style name="Widget.Fullwidth">
		<item name="android:layout_width">match_parent</item>
	</style>

	<style name="Widget.Fullscreen" parent="Widget.Fullwidth">
		<item name="android:layout_height">match_parent</item>
	</style>

	<style name="Widget.Image">
		<item name="android:scaleType">centerInside</item>
	</style>

	<style name="Widget.Image.Action">
		<item name="android:background">@drawable/bg_action</item>
		<item name="android:layout_width">@dimen/ic_action_width</item>
		<item name="android:layout_height">@dimen/ic_action_height</item>
	</style>

	<style name="Widget.Image.Action.Send">
		<item name="android:src">@drawable/ic_action_send</item>
	</style>

	<style name="Widget.Image.Delivered">
		<item name="android:src">@drawable/ic_action_message_delivered</item>
		<item name="android:layout_width">@dimen/ic_inline_width</item>
		<item name="android:layout_height">@dimen/ic_inline_height</item>
	</style>

	<style name="Widget.Text.__Error">
		<item name="android:drawableLeft">@drawable/ic_warning</item>
	</style>

	<style name="Widget.Text.Error" parent="Widget.Text.__Error">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_margin">@dimen/space_normal</item>
		<item name="android:paddingLeft">@dimen/space_normal</item>
		<item name="android:paddingRight">@dimen/space_normal</item>
		<item name="android:paddingTop">@dimen/space_normal</item>
		<item name="android:paddingBottom">@dimen/space_small</item>
		<item name="android:drawablePadding">@dimen/space_normal</item>
		<item name="android:background">@drawable/bg_card_error_below</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:shadowColor">@color/silent_black</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">1</item>
	</style>

	<style name="Widget.Text.Error.__Resend">
		<item name="android:drawableRight">@drawable/ic_action_send</item>
	</style>

	<style name="Widget.Text.Error.Resend" parent="Widget.Text.Error.__Resend">
		<item name="android:text">@string/notify_resend_request</item>
	</style>

	<style name="Widget.FloatingAction">
		<item name="android:background">@drawable/bg_floating_action</item>
		<item name="android:textColor">@color/silent_white</item>
		<item name="android:layout_margin">@dimen/space_normal</item>
		<item name="android:padding">@dimen/space_normal</item>
		<item name="android:gravity">center</item>
		<item name="android:scaleType">centerInside</item>
	</style>

	<style name="Widget.Layout.__FloatingActions" parent="Widget.Layout.Linear.Horizontal">
		<item name="android:layout_alignParentBottom">true</item>
		<item name="android:padding">@dimen/space_normal</item>
	</style>

	<style name="Widget.Layout.FloatingActions" parent="Widget.Layout.__FloatingActions">
		<item name="android:layout_alignParentRight">true</item>
	</style>

</resources>
